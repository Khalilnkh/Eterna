<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
        <div class="container">

            <ol>
                <li><a href="index.html">Home</a></li>
                <li>Team</li>
            </ol>
            <h2>Team</h2>

        </div>
    </section><!-- End Breadcrumbs -->
    <!-- ======= Team Section ======= -->
    <section id="team" class="team">
        <div class="container">

            <div class="row">
                @foreach (TeamTrainer item in Model)
                {
                    <div class="col-lg-4 col-md-6 d-flex align-items-stretch">
                        <div class="member">
                            <img src="~/assets/img/team/@item.Image" alt="">
                            <h4>@item.Name @item.Surname</h4>
                            <span>@item.TeamPosition.Name</span>
                            <p>
                                @item.Description
                            </p>
                            <div class="social">
                                <a href="@item.FacebookUrl"><i class="bi bi-twitter"></i></a>
                                <a href="@item.TwitterUrl"><i class="bi bi-facebook"></i></a>
                                <a href="@item.InstagramUrl"><i class="bi bi-instagram"></i></a>
                                <a href="@item.LinkedinUrl"><i class="bi bi-linkedin"></i></a>
                            </div>
                        </div>
                    </div>


                }

            </div>

        </div>
    </section><!-- End Team Section -->

</main><!-- End #main -->